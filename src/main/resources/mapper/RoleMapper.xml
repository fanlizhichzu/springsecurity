<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zjzhd.springsecurity.dao.RoleDao">

    <select id="queryById" resultType="com.zjzhd.springsecurity.model.entity.Role">
        select
               *
        from
             t_role
        where
              id = #{id}
    </select>
</mapper>